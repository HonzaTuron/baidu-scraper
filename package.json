{
  "name": "baidu-scraper",
  "version": "0.0.1",
  "description": "Apify scraper for Baidu search engine",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc-alias",
    "start": "node --experimental-specifier-resolution=node dist/main.js",
    "buildAndRun": "rm -rf dist crawlee_storage && npm run build && npm run start",
    "publish": "npm run build && apify push"
  },
  "keywords": [
    "apify"
  ],
  "author": "Jan TuroÅˆ",
  "license": "ISC",
  "dependencies": {
    "@apify/tsconfig": "^0.1.0",
    "@crawlee/playwright": "^3.0.3",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "crawlee": "^3.0.3",
    "playwright": "^1.25.0",
    "tsc-alias": "^1.7.0",
    "typescript": "^4.7.4"
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^17.0.41",
    "apify-cli": "^0.7.4",
    "husky": "^8.0.1",
    "prettier": "^2.7.1"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.2"
  },
  "engines": {
    "node": ">=16"
  }
}
